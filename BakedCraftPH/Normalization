FD :{ 22->23 to 27} shareHolder Form(BCNF) CK:{22}
CREATE TABLE "ShareHolder" (
  "PK_shareholder" NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "First" VARCHAR2(50),
  "Middle" VARCHAR2(50),
  "Last" VARCHAR2(50),
  "contact" VARCHAR2(13),
  "email" VARCHAR2(40)
);

FD :{28 ->22, 29, 30, 31 } share Form(BCNF) CK:{28}
CREATE TABLE "Share" (
  "PK_share" VARCHAR2(10) NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "Shared" NUMBER(10,2),
  "FK_shareholder" NUMBER NOT NULL,
  "Date" TIMESTAMP DEFAULT SYSTIMSTAMP NOT NULL ,
  "description" VARCHAR2(255),
  FOREIGN KEY ("FK_shareholder") REFERENCES "ShareHolder"("PK_shareholder")
);

FD :{32 -> 33,34,35 } MaterialStats Form(BCNF) CK:{32}
CREATE TABLE "MaterialStats" (
  "Stock" NUMBER,
  "Description" VARCHAR2(255),
  "Status" VARCHAR2(10),
  "PK_material" NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY
);

FD :{ 36,-> 28, 32, 37 to 42 } MaterialsAdded Form(BCNF) CK:{36}
CREATE TABLE "MaterialsAdded" (
  "PK_materialAdded" VARCHAR2(10) NUMBER GENERATED BY DEFAULT AS IDENTITY  PRIMARY KEY,
  "FK_material" NUMBER NOT NULL ,
  "description" VARCHAR2(255),
  "quantity" NUMBER,
  "price" NUMBER(10,2),
  "unit" VARCHAR2(50),
  "date" DATE DEFAULT SYSDATE NOT NULL ,
  "FK_share" NUMBER NOT NULL,
  FOREIGN KEY ("FK_share") REFERENCES "ShareHolder"("PK_shareholder"),
  FOREIGN KEY ("FK_material") REFERENCES "MaterialStats"("PK_material")
);

FD :{ 344 -> 28, 45 to 48 }  Neccesity Form(BCNF) CK:{44}
CREATE TABLE "Neccesity" (
  "PK_necessity" NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "necessity" VARCHAR2(50),
  "price" NUMBER(10,2),
  "description" VARCHAR2(255),
  "date" DATE DEFAULT SYSDATE NOT NULL ,
  "FK_share" NUMBER NOT NULL,
  FOREIGN KEY ("FK_share") REFERENCES "ShareHolder"("PK_shareholder")
);

FD :{ 42 -> 36, 43 } expensesLog Form(BCNF) CK:{42}
CREATE TABLE "ExpensesLog" (
  "PK_expenses" NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "date" DATE DEFAULT SYSDATE NOT NULL ,
  "FK_expenses_Neccesity_material" NUMBER NOT NULL,
  FOREIGN KEY ("FK_expenses_Neccesity_material") REFERENCES "Neccesity"("PK_necessity"),
  FOREIGN KEY ("FK_expenses_Neccesity_material") REFERENCES "MaterialsAdded"("PK_materialAdded")
);

FD :{ 18 ->22, 19, 20, 21 } paymentMethod Form(BCNF) CK:{18}
CREATE TABLE "PaymentMethod" (
  "PK_paymentMethod" NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "paymentMethod" VARCHAR2(50),
  "description" VARCHAR2(255),
  "discountRate" NUMBER(2,2),
  "PK_shareholder" NUMBER NOT NULL,
  FOREIGN KEY ("PK_shareholder") REFERENCES "ShareHolder"("PK_shareholder")
);

FD :{ 1 -> 2 to 62 }  Products Form(BCNF) CK:{1}
CREATE TABLE "Products" (
  "PK_productName" NUMBER GENERATED BY DEFAULT AS IDENTITY NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "desription" VARCHAR2(255),
  "price" NUMBER(10,2),
  "stock" NUMBER,
  "unit" VARCHAR2(25),
  "status" NUMBER(1) DEFAULT 1 CHECK ("status" IN (0, 1 ))
);

FD :{  7->1, 8 to 11  } ProductMade Form(BCNF) CK:{7}
CREATE TABLE "ProductMade" (
  "PK_productsLog" NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "FK_productName" NUMBER NOT NULL,
  "Date" DATE DEFAULT SYSDATE NOT NULL ,
  "quantitty" NUMBER,
  "description" VARCHAR2(255),
  "status" NUMBER(1) DEFAULT 1 CHECK ("status" IN (0, 1 )),
  FOREIGN KEY ("FK_productName") REFERENCES "Products"("PK_productName")
);

FD :{ 1, 32 -> 1, 32 } MaterialNeeded Form(BCNF) CK:{1,32}
CREATE TABLE "MaterialNeeded" (
  "FK_productName" NUMBER NOT NULL,
  "FK_material" NUMBER NOT NULL,
  FOREIGN KEY ("FK_productName") REFERENCES "ProductMade"("FK_productName"),
  FOREIGN KEY ("FK_productName") REFERENCES "Products"("PK_productName"),
  FOREIGN KEY ("FK_material") REFERENCES "MaterialStats"("PK_material")
);

FD :{  12 ->1, 13, 14  }  Orders Form(BCNF) CK:{12}
CREATE TABLE "Orders" (
  "PK_Order" NUMBER GENERATED BY DEFAULT AS IDENTITY NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "orderDate" TIMESTAMP DEFAULT SYSTIMSTAMP NOT NULL ,
  "status" NUMBER(1) DEFAULT 1 CHECK ("status" IN (1, 0)),
  "FK_paymentMethod" NUMBER NOT NULL,
  FOREIGN KEY ("FK_paymentMethod") REFERENCES "PaymentMethod"("PK_paymentMethod")
);

FD :{  15 ->12, 18, 1, 16, 17 } orderDetails Form(BCNF) CK:{15}
CREATE TABLE "orderDetails" (
  "PK_orderDetails" NUMBER GENERATED BY DEFAULT AS IDENTITY NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
  "FK_orderID" NUMBER NOT NULL,
  "FK_productName" NUMBER NOT NULL,
  "quantity" NUMBER,
  "status" NUMBER(1) DEFAULT 1 CHECK ("status" IN (1,0)),
  FOREIGN KEY ("FK_productName") REFERENCES "Products"("PK_productName"),
  FOREIGN KEY ("FK_orderID") REFERENCES "Orders"("PK_Order")
);
