FarmersInfo
R:{
1	2	3	4	5	6	7	8
}
FD :{ 1 -> 2 to 8} Highest Normal Form(BCNF) CK:{1}
CREATE TABLE [FarmersInfo] (
  [PK_farmersInfo] INT,
  [firstName] NVARCHAR(100),
  [lastName] NVARCHAR((100,
  [middleName] Nvarchar(100),
  [Birthday] Date,
  [Block] Nvarchar(50),
  [Street] Nvarchar(50),
  [baranggay] Nvarchar(5)
);


